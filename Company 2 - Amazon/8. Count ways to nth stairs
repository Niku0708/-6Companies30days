class Solution
{
    public:
    //Function to count number of ways to reach the nth stair 
    //when order does not matter.
    long long countWays(int m)
    {
        int ans = 1;
        ans += m/2;
        // while(m>1)
        // {
        //     m -= 2;
        //     ans++;
        // }
        return ans;
    }
};
